<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.tipolicencias">

    <Button
        android:id="@+id/temporal"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="60dp"
        android:background="@drawable/btn1"
        android:text="Temporal"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/indeter"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:background="@drawable/btn1"
        android:text="Indeterminada"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/temporal" />

    <Button
        android:id="@+id/itse"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:background="@drawable/btn1"
        android:text="ITSE"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/indeter" />

    <Button
        android:id="@+id/ecse"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:background="@drawable/btn1"
        android:text="ECSE"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itse" />

    <Button
        android:id="@+id/construc"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:background="@drawable/btn1"
        android:text="Construccion"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ecse" />

    <Button
        android:id="@+id/habilitacion"
        android:layout_width="280dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="50dp"
        android:background="@drawable/btn1"
        android:text="Habilitacion"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/construc" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="30dp"
        android:text="Escoge el tipo de licencia"
        android:textAlignment="gravity"
        android:textAllCaps="false"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topAppBar5_1" />


    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/topAppBar5_1"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:navigationIcon="@drawable/baseline_arrow_left_24"
        app:title="Volver"
        app:titleTextAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"

        app:titleTextColor="@color/atecolor"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>